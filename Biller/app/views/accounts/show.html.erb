<h1><%= @account.name %></h1>

<h3>Account Entries</h3>

<table>
	<% @account.account_entries.each do |e| %>
	<tr>
		<td><%= e.inspect %></td>
	</tr>
	<% end %>
</table>
<% if @acount.is_a?(Employee) %>
	<h3>Time Entries</h3>

	<table>
		<% @account.time_entries.each do |t| %>
		<tr>
			<td><%= t.inspect %></td>
		</tr>
		<% end %>
	</table>
<% end %>